{
  "expo": {
    "name": "健康家計簿",
    "slug": "FitnessTrackerApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/fitness-tracker-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/fitness-tracker-icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#007AFF"
    },
    "ios": {
      "supportsTablet": false,
      "icon": "./assets/images/fitness-tracker-icon.png",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
